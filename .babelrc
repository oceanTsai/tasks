{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "last 4 versions, not ie < 8",
                "useBuiltIns":"false",
            },
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }],
        ["@babel/plugin-transform-async-to-generator"]
    ]
}